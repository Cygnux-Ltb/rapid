@namespace("io.cygnuxltb.jcts.core.ser")
protocol TraderProtocol {

	@namespace("io.cygnuxltb.jcts.core.ser.enums")
	enum OrdType {
		// 无效
		INVALID,
		// 限价单
		LIMITED,
		// 市价单
		MARKET,
		// Limited Stop, 在目前的市场价格达到指定的止损价格时, 被激活成为限价单的报单.
		LIMITED_STOP,
		// Market Stop, 在目前的市场价格达到指定的止损价格时, 被激活成为市价单的报单.
		MARKET_STOP,
		// Market To Limited, 按照市价报单的方式成交, 不能成交的部分保留在报单队列中, 变成限价单的报单.
		MTL,
		// Best Price, 不带有价格限定, 按照市场中存在的最好价格买入或者卖出的报单.
		BP,
		// Average Price, 限定最终成交平均价格的报单.
		AP,
		// Kill Or Kill, 表示要求立即全部成交, 否则就全部取消的报单.
		FOK,
		// Fill And Kill, 表示要求立即成交, 对于无法满足的部分予以取消的报单.
		FAK,
		// Minimum Volume, 要求满足成交量达到这个最小成交量, 否则就取消的报单.
		MV
	}

	@namespace("io.cygnuxltb.jcts.core.ser.enums")
	enum OrdValid {
		//Good Till Cancel, 将一直有效, 直到交易员取消这个报单, 或者该合约本身到期的报单.
		GTC,
		//Good Till Date, 将一直有效, 直到指定日期或交易员取消这个报单, 或者该合约本身到期的报单.
		GTD,
		//Good For Day, 只在当日的交易时段有效, 一旦当前交易时段结束, 自动取消的报单.
		GFD
	}

	@namespace("io.cygnuxltb.jcts.core.ser.enums")
	enum OrdSide {
		//无效
		INVALID,
		//买
		BUY,
		//卖
		SELL,
		//融资买入
		MARGIN_BUY,
		//融券卖出
		SHORT_SELL
	}

	@namespace("io.cygnuxltb.jcts.core.ser.enums")
	enum OrdStatus {
		// 无效
		INVALID,
		// 新订单未确认
		PENDING_NEW,
		// 新订单
		NEW,
		// 新订单已拒绝
		NEW_REJECTED,
		// 部分成交
		PARTIALLY_FILLED,
		// 全部成交
		FILLED,
		// 未确认撤单
		PENDING_CANCEL,
		// 已撤单
		CANCELED,
		// 撤单已拒绝
		CANCEL_REJECTED,
		// 未确认修改订单
		PENDING_REPLACE,
		// 已修改
		REPLACED,
		// 已暂停
		SUSPENDED,
		// 未提供
		UNPROVIDED
	}

	@namespace("io.cygnuxltb.jcts.core.ser.enums")
	enum OrdLevel {
		//子订单
		CHILD,
		//父订单
		PARENT,
		//策略订单
		STRATEGY,
		//组订单
		GROUP
	}

	@namespace("io.cygnuxltb.jcts.core.ser.enums")
	enum TrdDirection {
		INVALID,
		LONG,
		SHORT
	}

	@namespace("io.cygnuxltb.jcts.core.ser.enums")
	enum TrdAction {
		// 无效
		INVALID,
		// 开仓
		OPEN,
		// 平仓
		CLOSE,
		// 平今仓
		CLOSE_TODAY,
		// 平昨仓
		CLOSE_YESTERDAY
	}

	@namespace("io.cygnuxltb.jcts.core.ser.enums")
	enum AdaptorStatus {
		// 无效
		INVALID,
		// 不可用
		UNAVAILABLE,
		// 行情启用
		MD_ENABLE,
		// 行情禁用
		MD_DISABLE,
		// 交易启用
		TRADER_ENABLE,
		// 交易禁用
		TRADER_DISABLE
	}

	/**
	 * 新订单
	 */
	@namespace("io.cygnuxltb.jcts.core.ser.req")
	record NewOrder {
		// Epoch Microsecond Unit
		long generateTime;
		// Epoch Microsecond Unit
		long sendTime;
		long ordSysId;
		int offerQty;
		int minimumQty = 1;
		double offerPrice = 0.0D;
		string exchangeCode;
		int instrumentId;
		string instrumentCode;
		io.cygnuxltb.jcts.core.ser.enums.OrdType type;
		io.cygnuxltb.jcts.core.ser.enums.OrdValid valid;
		io.cygnuxltb.jcts.core.ser.enums.TrdAction action;
		io.cygnuxltb.jcts.core.ser.enums.TrdDirection direction;
		string brokerId;
		int accountId;
		int subAccountId;
		int strategyId;
		string operatorId;
		string msg = "";
	}

	/**
	 * 撤单
	 */
	@namespace("io.cygnuxltb.jcts.core.ser.req")
	record CancelOrder {
		// Epoch Microsecond Unit
		long generateTime;
		// Epoch Microsecond Unit
		long sendTime;
		long ordSysId;
		boolean immediately = true;
		long delayMillis = 0;
		string exchangeCode;
		string instrumentCode;
		string brokerId;
		int accountId;
		int subAccountId;
		int strategyId;
		string operatorId = "";
		string msg = "";
	}

	/**
	 * 查询订单
	 */
	@namespace("io.cygnuxltb.jcts.core.ser.req")
	record QueryOrder {
		// Epoch Millisecond Unit
		long generateTime = 0;
		// Epoch Millisecond Unit
		long sendTime = 0;
		long ordSysId;
		string exchangeCode;
		string instrumentCode;
		string brokerId;
		int accountId;
		int subAccountId;
		int strategyId;
		string operatorId = "";
	}

	/**
	 * 查询账户持仓
	 */
	@namespace("io.cygnuxltb.jcts.core.ser.req")
	record QueryPositions {
		// Epoch Millisecond Unit
		long generateTime = 0;
		// Epoch Millisecond Unit
		long sendTime = 0;
		string exchangeCode;
		string instrumentCode;
		string brokerId;
		int accountId;
		int subAccountId;
		int strategyId;
		string operatorId = "";
	}

	/**
	 * 查询账户余额
	 */
	@namespace("io.cygnuxltb.jcts.core.ser.req")
	record QueryBalance {
		// Epoch Millisecond Unit
		long generateTime = 0;
		// Epoch Millisecond Unit
		long sendTime = 0;
		string brokerId;
		int accountId;
		int subAccountId;
		int strategyId;
		string operatorId = "";
	}

	/**
	 * 订单回报
	 */
	@namespace("io.cygnuxltb.jcts.core.ser.event")
	record OrderEvent {
		// Epoch Microsecond Unit
		long epochMicros;
		long ordSysId;
		int tradingDay;
		string brokerId;
		string investorId = "";
		string orderRef;
		string brokerOrdSysId;
		string exchangeCode;
		string instrumentCode;
		io.cygnuxltb.jcts.core.ser.enums.OrdStatus status;
		io.cygnuxltb.jcts.core.ser.enums.TrdDirection direction;
		io.cygnuxltb.jcts.core.ser.enums.TrdAction action;
		int offerQty = 0;
		int filledQty = 0;
		long offerPrice = 0;
		long tradePrice = 0;
		string offerTime = "";
		string updateTime = "";
		string msg = "";
	}

	/**
	 * 账户可用余额回报
	 */
	@namespace("io.cygnuxltb.jcts.core.ser.event")
	record BalanceEvent {
		long epochMillis;
		string brokerId;
		string investorId;
		long available;
		string currencyId;
	}

	/**
	 * 持仓回报
	 */
	@namespace("io.cygnuxltb.jcts.core.ser.event")
	record PositionsEvent {
		long epochMillis;
		string brokerId;
		string investorId;
		string exchangeCode;
		string instrumentCode;
		int qty;
	}

	/**
	 * 适配器回报
	 */
	@namespace("io.cygnuxltb.jcts.core.ser.event")
	record AdaptorEvent {
		long epochMillis;
		string adaptorId;
		int accountId;
		io.cygnuxltb.jcts.core.ser.enums.AdaptorStatus status;
	}

}
