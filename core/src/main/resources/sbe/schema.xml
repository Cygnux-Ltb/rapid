<?xml version="1.0" encoding="UTF-8"?>
<sbe:messageSchema xmlns:sbe="http://fixprotocol.io/2016/sbe"
                   package="io.rapid.core.sbe" id="1" version="0" semanticVersion="5.2"
                   description="Protocol types with Rapid Core">
    <types>
        <!-- ######### 数据类型 ######### -->
        <!-- #Epoch -->
        <type name="Epoch" primitiveType="uint64"/>
        <!-- #SysId -->
        <type name="SysId" primitiveType="uint64"/>
        <!-- #价格(Long) -->
        <type name="PriceL" primitiveType="int64"/>
        <!-- #价格(Double) -->
        <type name="PriceD" primitiveType="double"/>
        <!-- #数量 -->
        <type name="Qty" primitiveType="int32"/>
        <!-- #版本 -->
        <type name="Version" primitiveType="float"/>
        <!-- ExchangeCode -->
        <type name="ExchangeCode" primitiveType="char" length="8"/>
        <!-- InstrumentCode -->
        <type name="InstrumentCode" primitiveType="char" length="12"/>
        <!-- BrokerCode -->
        <type name="BrokerCode" primitiveType="char" length="12"/>
        <!-- ######### 枚举类型 ######### -->
        <!-- #订单类型 -->
        <enum name="OrdType" encodingType="int32">
            <!-- 无效 -->
            <validValue name="INVALID">-1</validValue>
            <!-- 限价单 -->
            <validValue name="LIMITED">1</validValue>
            <!-- 市价单 -->
            <validValue name="MARKET">2</validValue>
            <!-- Limited Stop, 在目前的市场价格达到指定的止损价格时, 被激活为限价单. -->
            <validValue name="LIMITED_STOP">3</validValue>
            <!-- Market Stop, 在目前的市场价格达到指定的止损价格时, 被激活为市价单. -->
            <validValue name="MARKET_STOP">4</validValue>
            <!-- Market To Limited, 按照市价报单的方式成交, 不能成交的部分保留在报单队列中, 变成限价单. -->
            <validValue name="MTL">5</validValue>
            <!-- Best Price, 不限定价格, 按照市场中存在的最好价格买入或者卖出. -->
            <validValue name="BP">6</validValue>
            <!-- Average Price, 限定最终成交平均价格. -->
            <validValue name="AP">7</validValue>
            <!-- Kill Or Kill, 表示要求立即全部成交, 否则全部取消. -->
            <validValue name="FOK">8</validValue>
            <!-- Fill And Kill, 表示要求立即成交, 对于无法满足的部分予以取消. -->
            <validValue name="FAK">9</validValue>
            <!-- Minimum Volume, 要求满足成交量达到最小成交量, 否则就取消. -->
            <validValue name="MV">10</validValue>
        </enum>
        <!-- #订单有效类型 -->
        <enum name="OrdValid" encodingType="int32">
            <!-- 无效 -->
            <validValue name="INVALID">-1</validValue>
            <!-- Good Till Cancel, 将一直有效, 直到交易员取消这个报单, 或者该合约本身到期的报单. -->
            <validValue name="GTC">1</validValue>
            <!-- Good Till Date, 将一直有效, 直到指定日期或交易员取消这个报单, 或者该合约本身到期的报单. -->
            <validValue name="GTD">2</validValue>
            <!-- Good For Day, 只在当日的交易时段有效, 一旦当前交易时段结束, 自动取消的报单. -->
            <validValue name="GFD">3</validValue>
        </enum>
        <!-- 买入卖出类型 -->
        <enum name="OrdSide" encodingType="int32">
            <!-- 无效 -->
            <validValue name="INVALID">-1</validValue>
            <!-- 买入 -->
            <validValue name="BUY">1</validValue>
            <!-- 卖出 -->
            <validValue name="SELL">2</validValue>
            <!-- 融资买入 -->
            <validValue name="MARGIN_BUY">3</validValue>
            <!-- 融券卖出 -->
            <validValue name="SHORT_SELL">4</validValue>
        </enum>
        <!-- #订单状态 -->
        <enum name="OrdStatus" encodingType="int32">
            <!-- 无效 -->
            <validValue name="INVALID">-1</validValue>
            <!-- 新订单未确认 -->
            <validValue name="PENDING_NEW">10</validValue>
            <!-- 新订单 -->
            <validValue name="NEW">11</validValue>
            <!-- 新订单已拒绝 -->
            <validValue name="NEW_REJECTED">12</validValue>
            <!-- 部分成交 -->
            <validValue name="PARTIALLY_FILLED">13</validValue>
            <!-- 全部成交 -->
            <validValue name="FILLED">14</validValue>
            <!-- 未确认撤单 -->
            <validValue name="PENDING_CANCEL">20</validValue>
            <!-- 撤单已拒绝 -->
            <validValue name="CANCEL_REJECTED">21</validValue>
            <!-- 已撤单 -->
            <validValue name="CANCELED">22</validValue>
            <!-- 未确认修改订单 -->
            <validValue name="PENDING_REPLACE">30</validValue>
            <!-- 修改已拒绝 -->
            <validValue name="REPLACE_REJECTED">31</validValue>
            <!-- 已修改 -->
            <validValue name="REPLACED">32</validValue>
            <!-- 已暂停 -->
            <validValue name="SUSPENDED">40</validValue>
            <!-- 未提供 -->
            <validValue name="UNPROVIDED">50</validValue>
        </enum>
        <!-- #订单级别 -->
        <enum name="OrdLevel" encodingType="int32">
            <!-- 子订单 -->
            <validValue name="CHILD">1</validValue>
            <!-- 父订单 -->
            <validValue name="PARENT">2</validValue>
            <!-- 策略订单 -->
            <validValue name="STRATEGY">3</validValue>
            <!-- 组订单 -->
            <validValue name="GROUP">4</validValue>
        </enum>
        <!-- #成交类型 -->
        <enum name="TrdDirection" encodingType="int32">
            <!-- 无效 -->
            <validValue name="INVALID">-1</validValue>
            <!-- 多 -->
            <validValue name="LONG">1</validValue>
            <!-- 空 -->
            <validValue name="SHORT">2</validValue>
        </enum>
        <!-- #成交动作类型 -->
        <enum name="TrdAction" encodingType="int32">
            <!-- 无效 -->
            <validValue name="INVALID">-1</validValue>
            <!-- 开仓 -->
            <validValue name="OPEN">1</validValue>
            <!-- 平仓 -->
            <validValue name="CLOSE">2</validValue>
            <!-- 平今仓 -->
            <validValue name="CLOSE_TODAY">3</validValue>
            <!-- 平昨仓 -->
            <validValue name="CLOSE_YESTERDAY">4</validValue>
        </enum>
        <!-- #Adaptor状态 -->
        <enum name="AdaptorStatus" encodingType="int32">
            <!-- 无效 -->
            <validValue name="INVALID">-1</validValue>
            <!-- 不可用 -->
            <validValue name="UNAVAILABLE">0</validValue>
            <!-- 可用 -->
            <validValue name="AVAILABLE">1</validValue>
            <!-- 行情启用 -->
            <validValue name="MD_ENABLE">10</validValue>
            <!-- 行情禁用 -->
            <validValue name="MD_DISABLE">11</validValue>
            <!-- 交易启用 -->
            <validValue name="TRADER_ENABLE">20</validValue>
            <!-- 交易禁用 -->
            <validValue name="TRADER_DISABLE">21</validValue>
        </enum>
        <!-- Self Add -->
        <composite name="messageHeader"
                   description="Message identifiers and length of message root.">
            <type name="blockLength" primitiveType="uint16"/>
            <type name="templateId" primitiveType="uint16"/>
            <type name="schemaId" primitiveType="uint16"/>
            <type name="version" primitiveType="uint16"/>
        </composite>
        <composite name="groupSizeEncoding"
                   description="Repeating group dimensions">
            <type name="blockLength" primitiveType="uint16"/>
            <type name="numInGroup" primitiveType="uint16"/>
        </composite>
        <composite name="utf8Encoding"
                   description="Variable length UTF-8 String">
            <type name="length" primitiveType="uint32" maxValue="1073741824"/>
            <type name="varData" primitiveType="uint8" length="0"
                  characterEncoding="UTF-8"/>
        </composite>
        <composite name="asciiEncoding"
                   description="Variable length ASCII String">
            <type name="length" primitiveType="uint32" maxValue="1073741824"/>
            <type name="varData" primitiveType="uint8" length="0"
                  characterEncoding="ASCII"/>
        </composite>
        <composite name="dataEncoding"
                   description="Variable length binary blob">
            <type name="length" primitiveType="uint32" maxValue="1073741824"/>
            <type name="varData" primitiveType="uint8" length="0"/>
        </composite>
    </types>
    <!-- Self Add End -->

    <sbe:message name="Order" id="1"
                 description="Sent Order Object">
        <field name="generateTime" id="1" type="Epoch"/>
        <field name="sendTime" id="2" type="Epoch"/>
        <field name="delayMillis" id="3" type="uint64"/>
        <field name="ordSysId" id="4" type="SysId"/>
        <field name="offerQty" id="5" type="int32"/>
        <field name="minTradingQty" id="6" type="int32"/>
        <field name="offerPrice" id="7" type="PriceD"/>
        <field name="exchangeCode" id="8" type="ExchangeCode"/>
        <field name="instrumentId" id="9" type="int32"/>
        <field name="instrumentCode" id="10" type="InstrumentCode"/>
        <field name="ordType" id="11" type="OrdType"/>
        <field name="ordValid" id="12" type="OrdValid"/>
        <field name="trdAction" id="13" type="TrdAction"/>
        <field name="trdDirection" id="14" type="TrdDirection"/>
        <field name="brokerCode" id="15" type="BrokerCode"/>
        <field name="accountId" id="16" type="int32"/>
        <field name="subAccountId" id="17" type="int32"/>
        <field name="strategyId" id="18" type="int32"/>
        <field name="algoId" id="19" type="int32"/>
        <field name="operatorId" id="20" type="int32"/>
        <field name="messageCode" id="21" type="int32"/>
    </sbe:message>
</sbe:messageSchema>
